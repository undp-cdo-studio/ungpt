{
  "name": "ungpt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --write --unsafe ./src",
    "test": "vitest --globals",
    "test:watch": "vitest",
    "build:watch": "tsc --noEmit --watch",
    "check": "biome check --write --unsafe --organize-imports-enabled true --formatter-enabled true ./src",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit --watch",
    "organize": "organize-imports-cli ./src/**/*.{ts,tsx}",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:spb": "supabase db reset",
    "db:rs": "rm -rf ./supabase/migrations && bun run db:generate && (supabase db reset || (supabase start && supabase db reset)) && bun run db:seed",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:seed:local": "./scripts/run-seed.sh",
    "db:seed:remote": "./scripts/run-seed-remote.sh",
    "db:setup": "npm run db:generate && npm run db:push && npm run db:seed",
    "db:all": "npm run db:generate && npm run db:push && npm run db:seed",
    "db:reset": "npm run db:generate && npm run db:push && npm run db:seed",
    "db:restart": "npm run db:generate && npm run db:seed",
    "db:verify": "tsx scripts/verify-db.ts",
    "db:safe-restart": "npm run clean && npm run db:generate && npm run db:seed",
    "db:schema": "npx --yes ts-json-schema-generator -p ./src/types/index.ts > types.json",
    "db:prod": "node -e \"require('dotenv').config(); if(!process.env.DATABASE_URL) { console.error('Error: DATABASE_URL is not set in the .env file.'); process.exit(1); }\" && drizzle-kit push:pg",
    "db:prod:seed": "cross-env NODE_ENV=production tsx scripts/seed-database.ts --prod",
    "db:prod:clean": "cross-env NODE_ENV=production tsx scripts/seed-database.ts --prod --clean"
  },
